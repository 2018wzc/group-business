<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lmgroup.groupbusiness.dao.HelloDao">
    <resultMap id="BaseResultMap" type="com.lmgroup.groupbusiness.domain.Hello">
        <id column="id" property="id" jdbcType="INTEGER"/>
        <result column="city" property="city" jdbcType="VARCHAR"/>
        <result column="num" property="num" jdbcType="INTEGER"/>
    </resultMap>
    <sql id="Base_Column_List">
        id, city, num
    </sql>
    <select id="selectHello" resultMap="BaseResultMap" parameterType="java.lang.Integer">
        select
        <include refid="Base_Column_List"/>
        from tsys_test
        where id = #{id,jdbcType=INTEGER}
    </select>


</mapper>